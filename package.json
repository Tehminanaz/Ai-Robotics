{
  "name": "physical-ai-humanoid-robotics-course",
  "version": "1.0.0",
  "description": "A comprehensive 12-week course and technical book on Physical AI & Humanoid Robotics",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run start",
    "dev:backend": "cd backend && python -m uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "build": "cd frontend && npm run build",
    "install:all": "npm install && cd frontend && npm install",
    "start:frontend": "cd frontend && npm run start",
    "start:backend": "cd backend && python -m uvicorn main:app --host 0.0.0.0 --port 8000"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "keywords": [
    "robotics",
    "ai",
    "physical-ai",
    "humanoid",
    "course",
    "education",
    "docusaurus",
    "fastapi",
    "rag"
  ],
  "author": "Claude Code",
  "license": "MIT"
}